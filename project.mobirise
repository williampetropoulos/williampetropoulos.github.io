        {
          "alias": "amp-blocks/components",
          "_isSecondary": true,
          "typeCompatible": "isAMP",
          "_styles": {
            "padding-top": "(@paddingTop * 1rem)",
            "padding-bottom": "(@paddingBottom * 1rem)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".link-items": {
              "@media (max-width: 992px)": {
                "justify-content": "space-between"
              },
              ".fLink": {
                "width": "auto",
                "line-height": "2"
              }
            },
            "& when (@showLine)": {
              ".footer-container": {
                "border-top": "4px solid @lineColor",
                "padding-top": "2rem"
              }
            },
            "& when (@typeBlock = '1')": {
              ".fLink": {
                "@media (min-width: 992px)": {
                  "padding-left": "2rem",
                  "padding-right": "2rem",
                  "@media (max-width: 992px)": {
                    "padding-left": "0.5rem",
                    "padding-right": "0.5rem"
                  },
                  "&:not(:nth-last-child(1))": {
                    "border-right": "2px solid @lineItemColor"
                  }
                }
              },
              ".mbr-row": {
                "margin": "0",
                "& when (@showCopyright)": {
                  "&:nth-child(1)": {
                    "margin-bottom": "1rem"
                  }
                }
              }
            },
            "& when (@typeBlock = '2')": {
              ".footer-container": {
                "& when (@showCopyright) and (@showItems)": {
                  "@media (min-width: 992px)": {
                    "justify-content": "space-between"
                  },
                  "@media (max-width: 991px)": {
                    "justify-content": "center"
                  }
                }
              },
              ".mbr-row": {
                "width": "auto",
                "@media (max-width: 992px)": {
                  "padding": "0",
                  "width": "100%",
                  "justify-content": "center",
                  "& when (@showCopyright)": {
                    "&:nth-child(1)": {
                      "margin-bottom": "1rem"
                    }
                  }
                }
              },
              ".copyright": {
                "width": "auto"
              }
            },
            "@media (max-width: 767px)": {
              ".link-items": {
                "-webkit-justify-content": "center",
                "justify-content": "center"
              }
            },
            ".copyright .mbr-text": {
              "color": "#ceb900"
            },
            ".fLink": {
              "color": "#ffffff"
            }
          },
          "_name": "ampFooter26",
