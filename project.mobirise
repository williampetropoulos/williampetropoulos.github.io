        {
          "alias": "amp-blocks/components",
          "_isSecondary": true,
          "typeCompatible": "isAMP",
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".contacts-box .contacts-text:not(.last-child) when (@showContacts)": {
              "padding-bottom": "5px"
            },
            ".img-box when (@showLogo), (@showBrand)": {
              "align-items": "center"
            },
            ".img-wrap when (@showLogo) and (@showBrand) and (@showLogoSocial)": {
              "margin-right": "15px"
            },
            "& when (@showForm)": {
              ".mbr-section-btn": {
                "padding-bottom": "0"
              },
              ".field-input": {
                "color": "@labelsColor",
                "&:not([type=\"checkbox\"]):not([type=\"radio\"])": {
                  "background": "contrast(@labelsColor, darken(@labelsColor, 50%), #ffffff, 60%) !important",
                  "border-color": "lighten(@labelsColor, 88%)",
                  "&:focus": {
                    "border-color": "@secondaryColor"
                  }
                }
              },
              "input::placeholder": {
                "color": "lighten(@labelsColor, 46%)"
              }
            },
            ".social .iconfont-wrapper when (@showSocial) and (@showLogoSocial)": {
              "line-height": "(@iconsSize + 0.25rem)",
              "width": "~\"@{iconsSize}rem\"",
              "height": "~\"@{iconsSize}rem\"",
              "border-color": "@iconsColor !important",
              "color": "contrast(@iconsColor, #222222, #ffffff, 15%)",
              "background": "@iconsColor !important",
              "& when ((@iconsSize * 16) > 30)": {
                "@media (max-width: 767px)": {
                  "width": "30px",
                  "height": "30px",
                  "line-height": "34px"
                }
              }
            },
            ".social .amp-iconfont when (@showSocial) and (@showLogoSocial)": {
              "font-size": "(@iconsSize * 0.42rem)",
              "line-height": "(@iconsSize - 0.25rem)",
              "width": "(@iconsSize * 0.42rem)",
              "color": "contrast(@iconsColor, #222222, #ffffff, 15%)",
              "& when ((@iconsSize * 16) > 30)": {
                "@media (max-width: 767px)": {
                  "font-size": "(30px * 0.42)",
                  "width": "(30px * 0.42)",
                  "line-height": "26px"
                }
              },
              "&:hover": {
                "color": "@primaryColor !important"
              }
            },
            ".form-block when (@transparentForm) and (@showForm)": {
              "padding": "0"
            },
            ".form-block .mbr-overlay when not (@transparentForm) and (@showForm)": {
              "background-color": "@formBgColor",
              "opacity": "@formBgOpacity"
            },
            ".img-wrap when (@showLogo) and (@showLogoSocial)": {
              "width": "~\"@{widthLogo}%\"",
              "min-width": "60px"
            },
            "@media (max-width: 991px)": {
              ".mbr-row > .md-pb": {
                "padding-bottom": "30px"
              }
            },
            "@media (max-width: 767px)": {
              ".mbr-row > :not(.last-child)": {
                "padding-bottom": "30px"
              },
              ".form-block .field when (@showForm)": {
                "padding-bottom": "0"
              }
            },
            ".mbr-section-title:after when (@showLineTitle) and (@showTitle)": {
              "content": "\"\"",
              "display": "block",
              "width": "40px",
              "height": "1px",
              "margin-top": "10px",
              "background-color": "@primaryColor",
              "& when (@align = '2')": {
                "margin-left": "auto",
                "margin-right": "auto"
              },
              "& when (@align = '3')": {
                "margin-left": "auto",
                "@media (max-width: 767px)": {
                  "margin-right": "auto"
                }
              }
            },
            ".mbr-list .mbr-text when (@showCategories)": {
              "line-height": "1",
              "&:before when (@showMarkers)": {
                "content": "''",
                "display": "inline-block",
                "vertical-align": "middle",
                "width": "~\"@{markersSize}rem\"",
                "height": "~\"@{markersSize}rem\"",
                "border-radius": "50%",
                "margin-right": "15px",
                "background-color": "@markersColor"
              },
              "& when (@showEffect)": {
                "transition": "all 0.3s",
                "&:hover": {
                  "padding-left": "20px",
                  "color": "@primaryColor"
                }
              }
            },
            ".mbr-list .mbr-text": {
              "text-align": "left"
            }
          },
          "_name": "ampFooter17",
          "_sourceTheme": "sportamp",
          "_customHTML": "<section class=\"ampFooter17\" group=\"Footers\" plugins=\"amp-main, amp-form, amp-mustache\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" condition=\"showTitle || (showContacts &amp;&amp; (showAddress || showPhone || showEmail)) || showCategories || (showForm &amp;&amp; (showTitle || showFormText)) || (showLogoSocial &amp;&amp; (showLogo || showSocial || showLogoText))\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"10\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"10\">\n        <input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\" condition=\"showTitle || (showContacts &amp;&amp; (showAddress || showPhone || showEmail)) || showCategories || (showForm &amp;&amp; (showTitle || showFormText)) || (showLogoSocial &amp;&amp; (showLogo || showSocial || showLogoText))\">\n    <header condition=\"showForm || showContacts || showCategories\">Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked=\"\" condition=\"showForm || showContacts || showCategories\">\n        <input type=\"checkbox\" title=\"Line Title\" name=\"showLineTitle\" condition=\" showTitle &amp;&amp; (showForm || showContacts || showCategories)\" checked=\"\">\n        <input type=\"checkbox\" title=\"Logo and Social\" name=\"showLogoSocial\" checked=\"\">\n        <input type=\"checkbox\" title=\"Contacts\" name=\"showContacts\" checked=\"\">\n        <input type=\"checkbox\" title=\"Categories\" name=\"showCategories\" checked=\"\">\n        <input type=\"checkbox\" title=\"Form\" name=\"showForm\" checked=\"\">\n        <select title=\"Align\" name=\"align\" condition=\"showTitle || (showContacts &amp;&amp; (showAddress || showPhone || showEmail)) || showCategories || (showForm &amp;&amp; (showTitle || showFormText)) || (showLogoSocial &amp;&amp; (showLogo || showSocial || showLogoText))\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected=\"\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n    <header condition=\"showLogoSocial\">Logo and Social</header>\n        <input type=\"checkbox\" name=\"showLogo\" title=\"Logo\" condition=\"showLogoSocial\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Width\" name=\"widthLogo\" min=\"10\" max=\"100\" value=\"10\" step=\"1\" condition=\"showLogoSocial &amp;&amp; showLogo\">\n        <input type=\"checkbox\" title=\"Show Brand Name\" name=\"showBrand\" condition=\"showLogoSocial\" checked=\"\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showLogoText\" checked=\"\" condition=\"showLogoSocial\"> \n        <input type=\"checkbox\" name=\"showSocial\" title=\"Social\" checked=\"\" condition=\"showLogoSocial\"> \n            <input type=\"range\" inline=\"\" title=\"Count\" name=\"socialCount\" min=\"1\" max=\"10\" value=\"3\" step=\"1\" condition=\"showSocial &amp;&amp; showLogoSocial\">\n            <input type=\"range\" inline=\"\" title=\"Size\" name=\"iconsSize\" min=\"1.5\" max=\"5\" step=\"0.1\" value=\"2.8\" condition=\"showSocial &amp;&amp; showLogoSocial\">\n            <input type=\"color\" title=\"Color\" name=\"iconsColor\" value=\"#4d4d4d\" condition=\"showSocial &amp;&amp; showLogoSocial\">\n    <header condition=\"showContacts\">Contacts</header>\n        <input type=\"checkbox\" name=\"showAddress\" title=\"Address\" checked=\"\" condition=\"showContacts\">\n        <input type=\"checkbox\" name=\"showPhone\" title=\"Phone\" checked=\"\" condition=\"showContacts\">\n        <input type=\"checkbox\" name=\"showEmail\" title=\"E-mail\" checked=\"\" condition=\"showContacts\">\n    <header condition=\"showCategories\">Categories</header>\n        <input type=\"range\" title=\"Count\" name=\"categoriesCount\" min=\"1\" max=\"10\" step=\"1\" value=\"8\" condition=\"showCategories\">\n        <input type=\"checkbox\" title=\"Markers\" name=\"showMarkers\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Size\" name=\"markersSize\" min=\"0.1\" max=\"1.5\" step=\"0.1\" value=\"0.5\" condition=\"showCategories &amp;&amp; showMarkers\">\n        <input type=\"color\" title=\"Color\" name=\"markersColor\" value=\"#ffffff\" condition=\"showMarkers &amp;&amp; showCategories\">\n        <input type=\"checkbox\" title=\"Show Effect Categorie\" name=\"showEffect\" checked=\"\">\n    <header condition=\"showForm\">Form</header>\n        <input type=\"checkbox\" title=\"Text\" name=\"showFormText\" condition=\"showForm\" checked=\"\">  \n        <input type=\"color\" title=\"Form Text Color\" name=\"labelsColor\" value=\"#000000\" condition=\"showForm\">\n        <input type=\"checkbox\" title=\"Transparent\" name=\"transparentForm\" condition=\"showForm\" checked=\"\">    \n            <input type=\"color\" title=\"Form Bg Color\" name=\"formBgColor\" value=\"#ffffff\" condition=\"!transparentForm &amp;&amp; showForm\">\n            <input type=\"range\" title=\"Form Bg Opacity\" name=\"formBgOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"1\" condition=\"!transparentForm &amp;&amp; showForm\">\n    <header>Background</header>\n        <input type=\"checkbox\" title=\"2 Background Color\" name=\"twoBgColor\" condition=\"bg.type == 'color'\"> \n        <input type=\"range\" inline=\"\" title=\"Width 2 Bg\" name=\"width2Bg\" min=\"1\" max=\"5\" step=\"1\" value=\"4\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">    \n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/background9.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#4d4d4d\" selected=\"\">\n        </fieldset>\n        <input type=\"color\" title=\"2 Color\" name=\"twoColor\" value=\"#f4f5f7\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">  \n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay &amp;&amp; bg.type !== 'color') || (twoBgColor &amp;&amp; bg.type == 'color')\" mbr-class=\"{'mbr-col-md-4': width2Bg == 1 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-5': width2Bg == 2 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-6': width2Bg == 3 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-7': width2Bg == 4 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-8': width2Bg == 5 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color'}\"></div>\n    <div mbr-class=\"{'container':!fullWidth,\n                     'container-fluid': fullWidth}\">\n        <div class=\"mbr-row mbr-jc-c\" mbr-class=\"{'mbr-px-2': fullWidth, 'mbr-row-reverse':reverseContent &amp;&amp; (showTitle || (showContacts &amp;&amp; (showAddress || showPhone || showEmail)) || showCategories || (showForm &amp;&amp; (showTitle || showFormText)) || (showLogoSocial &amp;&amp; (showLogo || showSocial || showLogoText || showBrand))), 'align-center': align == 2, 'align-right': align == 3}\">\n            <div class=\"logoSocial-box mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-12 last-child': !showContacts &amp;&amp; !showCategories &amp;&amp; !showForm, 'mbr-col-lg-3 mbr-col-md-6 md-pb': showContacts &amp;&amp; showCategories &amp;&amp;  showForm, 'mbr-col-md-6': (showContacts &amp;&amp; !showCategories &amp;&amp; !showForm) || (!showContacts &amp;&amp; !showCategories &amp;&amp; showForm) || (!showContacts &amp;&amp; showCategories &amp;&amp; !showForm), 'mbr-col-md-4': (showContacts &amp;&amp; showCategories &amp;&amp; !showForm) || (showContacts &amp;&amp; !showCategories &amp;&amp; showForm) || (!showContacts &amp;&amp; showCategories &amp;&amp; showForm)}\" mbr-if=\"showLogoSocial\">\n                <div class=\"img-box mbr-flex\" mbr-if=\"showLogo || showBrand\" mbr-class=\"{'mbr-m-auto': align == 2 &amp;&amp; showBrand &amp;&amp; showLogo, 'mbr-ml-auto': align == 3 &amp;&amp; showBrand &amp;&amp; showLogo}\">\n                    <div class=\"img-wrap\" mbr-if=\"showLogo\" mbr-class=\"{'mbr-m-auto': align == 2 &amp;&amp; !showBrand, 'mbr-ml-auto': align == 3 &amp;&amp; !showBrand}\">\n                        <amp-img src=\"@PROJECT_PATH@/assets/images/member-instructor-120x120.jpeg\" layout=\"responsive\" width=\"59.99607252747252\" height=\"69.1094\" alt=\"United World Muay Thai Association\" class=\"mobirise-loader\">\n                            <div placeholder=\"\" class=\"placeholder\"></div>\n                            <a href=\"index.html\"><img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/member-instructor-120x120.jpeg\" alt=\"United World Muay Thai Association\" title=\"\"></a>\n                        </amp-img>\n                    </div>\n                    <span mbr-if=\"showBrand\" mbr-buttons=\"\" mbr-theme-style=\"display-5\" data-toolbar=\"-mbrBtnMove,-mbrBtnAdd,-mbrBtnRemove\"><a class=\"navbar-caption mbr-bold text-primary\" data-app-selector=\".navbar-caption\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">UWMTA.ORG</a></span>\n                </div>\n                <p class=\"mbr-text mbr-white mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".logoSocial-box .mbr-text\" mbr-if=\"showLogoText\" mbr-class=\"{'mbr-pt-2': showLogo || showBrand}\"><a href=\"privacypolicy.html#top\" class=\"text-primary\" style=\"\">Privacy Policy</a><br><a href=\"termsofuse.html#top\" class=\"text-primary\" style=\"\">Terms of Use</a><br><a href=\"cookie.html#top\" class=\"text-primary\" style=\"\">Cookie Policy</a></p>\n                <div class=\"icons-list social\" mbr-if=\"showSocial\" mbr-list=\"\" mbr-list-grow=\"socialCount\" mbr-link-type=\"socials\" mbr-class=\"{'mbr-pt-2': showLogoText || showLogo || showBrand}\">\n                    <a href=\"https://www.youtube.com/channel/UCAZuC0bouAFB_2xcGX-LQIA\" target=\"_blank\">\n                        <span class=\"iconfont-wrapper align-center rounded\">\n                            <span mbr-icon=\"\" class=\"amp-iconfont fa-youtube fa\"></span>\n                        </span>\n                    </a>\n                    <a href=\"https://twitter.com/unitedworldmuay\" target=\"_blank\">\n                        <span class=\"iconfont-wrapper align-center rounded\">\n                            <span mbr-icon=\"\" class=\"amp-iconfont fa-twitter fa\"></span>\n                        </span>\n                    </a>\n                    \n                    \n                <a href=\"https://www.facebook.com/uwmta/\" target=\"_blank\">\n                        <span class=\"iconfont-wrapper align-center rounded\">\n                            <span mbr-icon=\"\" class=\"amp-iconfont fa-facebook-square fa\"></span>\n                        </span>\n                    </a></div>\n            </div>\n            <div class=\"contacts-box mbr-col-sm-12 mbr-white\" mbr-class=\"{'mbr-col-md-12': !showLogoSocial &amp;&amp; !showCategories &amp;&amp; !showForm, 'mbr-col-lg-3 mbr-col-md-6 md-pb': showLogoSocial &amp;&amp; showCategories &amp;&amp; showForm, 'mbr-col-md-6': (showLogoSocial &amp;&amp; !showCategories &amp;&amp; !showForm) || (!showLogoSocial &amp;&amp; !showCategories &amp;&amp; showForm) || (!showLogoSocial &amp;&amp; showCategories &amp;&amp; !showForm), 'mbr-col-md-4': (showLogoSocial &amp;&amp; showCategories &amp;&amp; !showForm) || (showLogoSocial &amp;&amp; !showCategories &amp;&amp; showForm) || (!showLogoSocial &amp;&amp; showCategories &amp;&amp; showForm), 'last-child': !showCategories &amp;&amp; !showForm}\" mbr-if=\"showContacts\">\n                <h3 class=\"mbr-section-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-section-title\" mbr-if=\"showTitle\" mbr-class=\"{'mbr-pb-4': showAddress || showPhone || showEmail}\">CONTACT</h3>\n                <p class=\"mbr-text contacts-text mbr-fonts-style\" mbr-if=\"showAddress\" mbr-theme-style=\"display-7\" data-app-selector=\".contacts-text\" mbr-class=\"{'last-child': !showPhone &amp;&amp; !showEmail}\"><span mbr-link-type=\"address\">141 East 55th st, </span><br><span mbr-link-type=\"address\">New York, NY 10022</span></p>\n                <p class=\"mbr-text contacts-text mbr-fonts-style\" mbr-if=\"showPhone\" mbr-theme-style=\"display-7\" data-app-selector=\".contacts-text\" mbr-class=\"{'last-child': !showPhone}\"><span mbr-link-type=\"phone\">212-935-4441</span><br><span mbr-link-type=\"phone\"><br></span>Houston, Texas<br><span mbr-link-type=\"phone\">281-343-0500</span><br><span mbr-link-type=\"phone\">Need Help Email Us</span></p>\n                <p class=\"mbr-text contacts-text mbr-fonts-style last-child\" mbr-if=\"showEmail\" mbr-theme-style=\"display-7\" data-app-selector=\".contacts-text\"><span mbr-link-type=\"email\"><a href=\"mailto:support@uwmta.org\" class=\"text-primary\">support@uwmta.org</a></span></p>  \n            </div>\n            <div class=\"categories-box mbr-col-sm-12 mbr-white\" mbr-class=\"{'mbr-col-md-12': !showLogoSocial &amp;&amp; !showContacts &amp;&amp; !showForm, 'mbr-col-lg-3 mbr-col-md-6': showLogoSocial &amp;&amp; showContacts &amp;&amp; showForm, 'mbr-col-md-6': (showLogoSocial &amp;&amp; !showContacts &amp;&amp; !showForm) || (!showLogoSocial &amp;&amp; !showContacts &amp;&amp; showForm) || (!showLogoSocial &amp;&amp; showContacts &amp;&amp; !showForm), 'mbr-col-md-4': (showLogoSocial &amp;&amp; showContacts &amp;&amp; !showForm) || (showLogoSocial &amp;&amp; !showContacts &amp;&amp; showForm) || (!showLogoSocial &amp;&amp; showContacts &amp;&amp; showForm), 'last-child': !showForm}\" mbr-if=\"showCategories\">\n                <h3 class=\"mbr-section-title mbr-fonts-style mbr-bold mbr-pb-4\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-section-title\" mbr-if=\"showTitle\">Quick Links</h3>\n                <div class=\"mbr-list\" mbr-list=\"\" mbr-list-grow=\"categoriesCount\">\n                    <p class=\"mbr-text mbr-fonts-style mbr-py-1\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-list .mbr-text\"><a href=\"index.html\" class=\"text-primary\">&nbsp;Home</a></p>\n                    \n                    \n                <p class=\"mbr-text mbr-fonts-style mbr-py-1\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-list .mbr-text\"><a href=\"members.html\" class=\"text-primary\">&nbsp;Members</a></p><p class=\"mbr-text mbr-fonts-style mbr-py-1\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-list .mbr-text\"><a href=\"affiliates.html\" class=\"text-primary\">&nbsp;Affiliates</a></p><p class=\"mbr-text mbr-fonts-style mbr-py-1\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-list .mbr-text\"><a href=\"blog.html\" class=\"text-primary\">&nbsp;Blog</a></p><p class=\"mbr-text mbr-fonts-style mbr-py-1\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-list .mbr-text\"><a href=\"news.html\" class=\"text-primary\">&nbsp;News</a><br></p><p class=\"mbr-text mbr-fonts-style mbr-py-1\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-list .mbr-text\"><a href=\"history.html\" class=\"text-primary\">&nbsp;History</a></p><p class=\"mbr-text mbr-fonts-style mbr-py-1\" mbr-theme-style=