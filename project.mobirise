              }
            },
            "& when (@showLine)": {
              ".footer-container": {
                "border-top": "4px solid @lineColor",
                "padding-top": "2rem"
              }
            },
            "& when (@typeBlock = '1')": {
              ".fLink": {
                "@media (min-width: 992px)": {
                  "padding-left": "2rem",
