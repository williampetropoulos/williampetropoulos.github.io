          "_customHTML": "<section class=\"ampFeatures28\" group=\"Features\" plugins=\"amp-main\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n            <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"3\">\n            <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"3\">\n            <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n             <input type=\"checkbox\" title=\"Show Card 1\" name=\"showCard1\" checked=\"\">\n             <input type=\"checkbox\" title=\"Reverse Content\" name=\"reverse\">\n        <header>Image</header>\n            <input type=\"range\" inline=\"\" title=\"Image Height\" name=\"imgHeight\" min=\"300\" max=\"700\" step=\"50\" value=\"300\">\n            <input type=\"checkbox\" title=\"Image Effect\" name=\"imgEffect\" checked=\"\">\n            <input type=\"color\" title=\"Image Overlay Color 1\" name=\"imgOverlayColor1\" value=\"#4d4d4d\" condition=\"imgEffect\">\n            <input type=\"color\" title=\"Image Overlay Color 2\" name=\"imgOverlayColor2\" value=\"#4d4d4d\" condition=\"imgEffect &amp;&amp; showCard1\">\n            <input type=\"range\" inline=\"\" title=\"Image Overlay Opacity\" name=\"imgOverlayOpacity\" min=\"0\" max=\"0.9\" step=\"0.05\" value=\"0.35\" condition=\"imgEffect\">\n        <header>Text</header>\n            <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Text\" name=\"showText\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Button\" name=\"showButtons\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Icon\" name=\"showIcon\" checked=\"\">\n            <input type=\"color\" title=\"Icon Color\" value=\"#ceb900\" name=\"iconColor\" condition=\"showIcon\">\n            