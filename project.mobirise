        {
          "alias": "amp-blocks/components",
          "_isSecondary": true,
          "typeCompatible": "isAMP",
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            "amp-carousel .card": {
              "align-self": "flex-start"
            },
            ".slider-box, amp-carousel": {
              "height": "~\"@{slideHeight}px\" !important",
              "position": "relative"
            },
            ".card-box .card-subtitle when (@showCardsText) and (@showCardsTitle)": {
              "padding-top": "25px"
            },
            ".card-box when (@showCardsTitle), (@showCardsText)": {
              "padding-top": "45px"
            },
            ".card, .slider-box, amp-carousel": {
              "width": "100%"
            },
            ".dots span, .amp-carousel-button": {
              "background-color": "@buttonsColor",
              "border-color": "@buttonsColor"
            },
            "& when not (@typeSlide = 'carousel')": {
              ".card": {
                "height": "~\"calc(@{slideHeight}px - 35px)\"",
                "margin-bottom": "35px"
              },
              ".dots-wrapper when (@showBullets)": {
                "position": "absolute",
                "bottom": "0",
                "left": "50%",
                "transform": "translateX(-50%)"
              },
              ".amp-carousel-button": {
                "display": "none"
              }
            },
            "& when not (@showBullets), (@typeSlide = 'carousel')": {
              ".card when (@typeSlide = 'carousel'), (@slideWidth = 0), not (@showCenter)": {
                "height": "~\"calc(@{slideHeight}px - 85px)\"",
                "margin": "0 0 85px 0"
              },
              ".card when (@typeSlide = 'slides') and not (@slideWidth = 0) and (@showCenter)": {
                "height": "~\"@{slideHeight}px\""
              },
              ".amp-carousel-button": {
                "display": "block",
                "opacity": "0.8",
                "&:before, &:after": {
                  "color": "contrast(@buttonsColor, darken(@buttonsColor, 50%), #ffffff, 60%) !important"
                },
                "&:hover": {
                  "opacity": "1"
                }
              },
              ".amp-carousel-button when (@typeSlide = 'carousel'), (@slideWidth = 0), not (@showCenter)": {
                "top": "~\"calc(100% - 27px)\"",
                "&.amp-carousel-button-prev": {
                  "left": "15px"
                },
                "&.amp-carousel-button-next": {
                  "left": "70px"
                },
                "& when (@cardsAmount = 2) and (@typeSlide = 'carousel')": {
                  "@media (min-width: 1200px)": {
                    "&.amp-carousel-button-prev": {
                      "left": "30px"
                    },
                    "&.amp-carousel-button-next": {
                      "left": "(30px + 55px)"
                    }
                  }
                }
              },
              ".dots-wrapper": {
                "display": "none"
              },
              "& when not (@showBullets) and not (@typeSlide = 'carousel')": {
                ".card-box when (@showCenter)": {
                  "@media (min-width: 768px)": {
                    "padding-left": "60px",
                    "padding-right": "60px"
                  }
                },
                "@media (max-width: 767px)": {
                  ".dots-wrapper": {
                    "display": "flex"
                  },
                  ".amp-carousel-button": {
                    "display": "none"
                  },
                  ".card": {
                    "height": "~\"calc(@{slideHeight}px - 35px)\"",
                    "margin-bottom": "35px"
                  }
                }
              }
            },
            ".card-wrapper": {
              "height": "100%",
              "overflow": "hidden",
              ".card-box when (@showCardsTitle), (@showCardsText)": {
                "white-space": "normal"
              },
              "img": {
                "object-fit": "cover",
                "-webkit-transition": "transform 5s linear",
                "transition": "transform 5s linear"
              },
              "&:hover img": {
                "-webkit-transform": "scale(1.15)",
                "transform": "scale(1.15)"
              }
            },
            ".card-wrapper when not (@transparentBg)": {
              "background-color": "@cardBg"
            },
            ".card-wrapper when (@showBorders)": {
              "border": "1px solid @bordersColor"
            },
            ".card-img amp-img": {
              "z-index": "2",
              "width": "~\"@{imageWidth}%\"",
              "min-width": "60px"
            },
            ".card .card-subtitle:before when (@lineTitle) and (@showCardsTitle)": {
              "content": "''",
              "display": "block",
              "width": "40px",
              "height": "3px",
              "margin": "inherit",
              "margin-bottom": "30px",
              "background-color": "@primaryColor",
              "& when (@cardAlign = '3')": {
                "@media (max-width: 767px)": {
                  "margin-right": "auto"
                }
              }
            },
            ".card-text": {
              "color": "#ceb900"
            },
            ".card-subtitle": {
              "color": "#ffffff"
            }
          },
          "_name": "ampTestimonials2",
          "_customHTML": "<section class=\"ampTestimonials2 slider slider-with-bullets\" group=\"Testimonials\" plugins=\"amp-main, amp-carousel, amp-bind\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header mbr-if=\"showTitle\">Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Padding Slide\" name=\"slideWidth\" min=\"0\" max=\"5\" step=\"1\" value=\"3\" condition=\"typeSlide == 'slides'\">\n\n    <header>Title</header>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\">\n        <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\">\n        <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected=\"\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n\n    <header>Cards</header>\n        <input type=\"range\" inline=\"\" title=\"Count\" name=\"cardsAmount\" min=\"1\" max=\"4\" step=\"1\" value=\"3\" condition=\"typeSlide == 'carousel'\">\n        <select title=\"Align\" name=\"cardAlign\" condition=\"showCardsTitle || showCardsText\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected=\"\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <input type=\"checkbox\" title=\"Show Name\" name=\"showCardsTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Line Title\" name=\"lineTitle\" condition=\"showCardsTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showCardsText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Borders\" name=\"showBorders\">\n        <input type=\"color\" title=\"Color\" name=\"bordersColor\" value=\"#cccccc\" condition=\"showBorders\">\n        <input type=\"checkbox\" title=\"Transparent\" name=\"transparentBg\" checked=\"\">\n        <input type=\"color\" title=\"Color\" name=\"cardBg\" value=\"#f4f5f7\" condition=\"!transparentBg\">\n\n    <header>Image</header>\n        <input type=\"checkbox\" title=\"Rounded\" name=\"roundedImage\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Width\" name=\"imageWidth\" min=\"10\" max=\"100\" step=\"1\" value=\"18\">\n\n    <header>Sliders</header>\n        <input type=\"checkbox\" title=\"Autoplay\" name=\"autoplay\" checked=\"\" condition=\"slidersAmount > 1 &amp;&amp; (showImgText || showImgTitle || showImg)\">\n            <input type=\"range\" inline=\"\" title=\"Delay\" name=\"delay\" min=\"1000\" max=\"5000\" step=\"1000\" value=\"3000\" condition=\"autoplay &amp;&amp; slidersAmount > 1 &amp;&amp; (showImgText || showImgTitle || showImg)\">\n        <input type=\"range\" inline=\"\" title=\"Slider Height\" name=\"slideHeight\" min=\"250\" max=\"500\" step=\"10\" value=\"380\">\n        <select title=\"Type Slide\" name=\"typeSlide\">\n            <option value=\"slides\" selected=\"\">Slides</option>\n            <option value=\"carousel\">Carousel</option>\n        </select>\n        <select title=\"Sliders Count\" name=\"slidersAmount\">\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\" selected=\"\">5</option>\n            <option value=\"6\">6</option>\n            <option value=\"7\">7</option>\n            <option value=\"8\">8</option>\n        </select>\n        <input type=\"checkbox\" title=\"Show Buttons/Bullets\" name=\"showBullets\" condition=\"typeSlide == 'slides'\" checked=\"\">\n            <input type=\"color\" title=\"Color\" name=\"buttonsColor\" value=\"#ceb900\">\n        <input type=\"checkbox\" title=\"Show Center Buttons\" name=\"showCenter\" condition=\"typeSlide == 'slides' &amp;&amp; !showBullets\" checked=\"\">\n        \n    <header>Background</header>\n        <input type=\"checkbox\" title=\"2 Background Color\" name=\"twoBgColor\" condition=\"bg.type == 'color'\"> \n        <input type=\"range\" inline=\"\" title=\"Width 2 Bg\" name=\"width2Bg\" min=\"1\" max=\"5\" step=\"1\" value=\"3\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">    \n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background1.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#4d4d4d\" selected=\"\">\n        </fieldset>\n        <input type=\"color\" title=\"2 Color\" name=\"twoColor\" value=\"#f4f5f7\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">  \n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay &amp;&amp; bg.type !== 'color') || (twoBgColor &amp;&amp; bg.type == 'color')\" mbr-class=\"{'mbr-col-md-4': width2Bg == 1 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-5': width2Bg == 2 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-6': width2Bg == 3 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-7': width2Bg == 4 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-8': width2Bg == 5 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color'}\"></div>\n    <div mbr-class=\"{'container':!fullWidth,\n                     'container-fluid':fullWidth}\">\n        <div class=\"title-wrap mbr-pb-4\" mbr-if=\"showTitle || showSubtitle\" mbr-class=\"{'align-left': titleAlign==1,\n                         'align-center': titleAlign==2,\n                         'align-right': titleAlign==3, 'mbr-px-2': fullWidth}\">\n            <h3 class=\"mbr-section-title mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".title-wrap .mbr-section-title\">\n                Testimonials</h3>\n            <h4 class=\"mbr-section-subtitle mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\" data-app-selector=\".title-wrap .mbr-section-subtitle\">\n                Testimonials Subtitle</h4>\n        </div>\n        <div class=\"mbr-row mbr-jc-c container-card\" mbr-class=\"{'mbr-px-2': fullWidth &amp;&amp; typeSlide == 'carousel', 'cardsAmount2': cardsAmount == 2 &amp;&amp; typeSlide == 'carousel'}\">\n             <div class=\"slider-box mbr-col-md-12\" mbr-class=\"{'mbr-col-lg-10': slideWidth == 1 &amp;&amp; typeSlide != 'carousel', 'mbr-col-lg-8': slideWidth == 2 &amp;&amp; typeSlide != 'carousel', 'mbr-col-lg-7': slideWidth == 3 &amp;&amp; typeSlide != 'carousel', 'mbr-col-lg-6': slideWidth == 4 &amp;&amp; typeSlide != 'carousel', 'mbr-col-lg-5': slideWidth == 5 &amp;&amp; typeSlide != 'carousel', 'mbr-m-auto': typeSlide != 'carousel' &amp;&amp; slideWidth != 0}\" data-autoplay=\"{{autoplay}}\" data-delay=\"{{delay}}\"><amp-carousel class=\"carousel\" height=\"{{slideHeight}}\" layout=\"fixed-height\" type=\"{{typeSlide}}\" id=\"carousel1\" loop=\"\" on=\"slideChange:AMP.setState({selectedSlide: event.index})\"><div class=\"card mbr-col-sm-12 last-child\" mbr-class=\"{'mbr-col-md-6 mbr-col-lg-6':cardsAmount == 2 &amp;&amp; typeSlide == 'carousel', 'mbr-col-md-4':cardsAmount == 3 &amp;&amp; typeSlide == 'carousel', 'mbr-col-md-6 mbr-col-lg-3':cardsAmount == 4 &amp;&amp; typeSlide == 'carousel', 'mbr-col-md-12': cardsAmount == 1 ||  typeSlide != 'carousel'}\">\n                        <div class=\"card-wrapper\" mbr-class=\"{'align-center': cardAlign==2,\n                                 'align-right': cardAlign==3, 'mbr-p-4': cardsAmount == 4 &amp;&amp; (showBorders || !transparentBg), 'mbr-p-5': cardsAmount < 4 &amp;&amp; (showBorders || !transparentBg)}\">\n                            <div class=\"card-img\">\n                                <amp-img class=\"mobirise-loader\" mbr-class=\"{'rounded': roundedImage, 'mbr-m-auto': cardAlign==2, 'mbr-ml-auto': cardAlign==3}\" src=\"@PROJECT_PATH@/assets/images/mbr-216x144.jpg\" layout=\"responsive\" width=\"570\" height=\"380\" alt=\"Mobirise\">\n                                    <div placeholder=\"\" class=\"placeholder\"></div>\n                                    <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/mbr-216x144.jpg\" alt=\"Mobirise\">\n                                </amp-img>\n                            </div>\n                            <div class=\"card-box\" mbr-if=\"showCardsTitle || showCardsText\">\n                                <p class=\"card-text mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-7\" mbr-if=\"showCardsText\" data-app-selector=\".card-text\">I joined uwmta in 2017 and they have helped me connect with gyms and instructors in my area, I'm very thankful!</p>\n                                <h3 class=\"card-subtitle mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".card-subtitle\" mbr-class=\"{'mbr-m-auto': cardAlign==2, 'mbr-ml-auto': cardAlign==3 }\" mbr-if=\"showCardsTitle\">BRIANA SCOTT<br>LA, CA</h3>\n                            </div>\n                        </div>\n                    </div><div class=\"card last-child mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-6 mbr-col-lg-6':cardsAmount == 2 &amp;&amp; typeSlide == 'carousel', 'mbr-col-md-4':cardsAmount == 3 &amp;&amp; typeSlide == 'carousel', 'mbr-col-md-6 mbr-col-lg-3':cardsAmount == 4 &amp;&amp; typeSlide == 'carousel', 'mbr-col-md-12': cardsAmount == 1 ||  typeSlide != 'carousel', 'super-hide': slidersAmount < 2 &amp;&amp; typeSlide == 'carousel'}\" mbr-if=\"slidersAmount > 1\">\n                        <div class=\"card-wrapper\" mbr-class=\"{'align-center': cardAlign==2,\n                                 'align-right': cardAlign==3, 'mbr-p-4': cardsAmount == 4 &amp;&amp; (showBorders || !transparentBg), 'mbr-p-5': cardsAmount < 4 &amp;&amp; (showBorders || !transparentBg)}\">\n                            <div class=\"card-img\">\n                                <amp-img class=\"mobirise-loader\" mbr-class=\"{'rounded': roundedImage, 'mbr-m-auto': cardAlign==2, 'mbr-ml-auto': cardAlign==3}\" src=\"@PROJECT_PATH@/assets/images/mbr-4-216x144.jpg\" layout=\"responsive\" width=\"570\" height=\"380\" alt=\"Mobirise\">\n                                    <div placeholder=\"\" class=\"placeholder\"></div>\n                                    <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/mbr-4-216x144.jpg\" alt=\"Mobirise\">\n                                </amp-img>\n                            </div>\n                            <div class=\"card-box\" mbr-if=\"showCardsTitle || showCardsText\">\n                                <p class=\"card-text mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-7\" mbr-if=\"showCardsText\" data-app-selector=\".card-text\">I have been a member of the United World Muay Thai Association since the early 2000s' and a certified instructor. I'm grateful for all their help and guidance in finding a facility to train in my area and becoming a Khru.</p>\n                                <h3 class=\"card-subtitle mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".card-subtitle\" mbr-class=\"{'mbr-m-auto': cardAlign==2, 'mbr-ml-auto': cardAlign==3 }\" mbr-if=\"showCardsTitle\">JOE NEAL<br>NY</h3>\n                            </div>\n                        </div>\n                    </div><div class=\"card last-child mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-6 mbr-col-xl-6':cardsAmount == 2 &amp;&amp; typeSlide == 'carousel', 'mbr-col-md-4':cardsAmount == 3 &amp;&amp; typeSlide == 'carousel', 'mbr-col-md-6 mbr-col-lg-3':cardsAmount == 4 &amp;&amp; typeSlide == 'carousel', 'mbr-col-md-12': cardsAmount == 1 ||  typeSlide != 'carousel', 'super-hide': slidersAmount < 3 &amp;&amp; typeSlide == 'carousel'}\" mbr-if=\"slidersAmount > 2\">\n                        <div class=\"card-wrapper\" mbr-class=\"{'align-center': cardAlign==2,\n                                 'align-right': cardAlign==3, 'mbr-p-4': cardsAmount == 4 &amp;&amp; (showBorders || !transparentBg), 'mbr-p-5': cardsAmount < 4 &amp;&amp; (showBorders || !transparentBg)}\">\n                            <div class=\"card-img\">\n                                <amp-img class=\"mobirise-loader\" mbr-class=\"{'rounded': roundedImage, 'mbr-m-auto': cardAlign==2, 'mbr-ml-auto': cardAlign==3}\" src=\"@PROJECT_PATH@/assets/images/mbr-216x138.jpg\" layout=\"responsive\" width=\"594.7826086956522\" height=\"380\" alt=\"Mobirise\">\n                                    <div placeholder=\"\" class=\"placeholder\"></div>\n                                    <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/mbr-216x138.jpg\" alt=\"Mobirise\">\n                                </amp-img>\n                            </div>\n                            <div class=\"card-box\" mbr-if=\"showCardsTitle || showCardsText\">\n                                <p class=\"card-text mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-7\" mbr-if=\"showCardsText\" data-app-selector=\".card-text\">I have been teaching for many years and even though I did not train in a affiliate gym of the uwmta, they have helped me further my training and get certified from a reputable organization.</p>\n                                <h3 class=\"card-subtitle mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".card-subtitle\" mbr-class=\"{'mbr-m-auto': cardAlign==2, 'mbr-ml-auto': cardAlign==3 }\" mbr-if=\"showCardsTitle\">Frank Lixn<br>WA</h3>\n                            </div>\n                        </div>\n                    </div><div class=\"card last-child mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-6 mbr-col-xl-6':cardsAmount == 2 &amp;&amp; typeSlide == 'carousel', 'mbr-col-md-4':cardsAmount == 3 &amp;&amp; typeSlide == 'carousel', 'mbr-col-md-6 mbr-col-lg-3':cardsAmount == 4 &amp;&amp; typeSlide == 'carousel', 'mbr-col-md-12': cardsAmount == 1 ||  typeSlide != 'carousel', 'super-hide': slidersAmount < 4 &amp;&amp; typeSlide == 'carousel'}\" mbr-if=\"slidersAmount > 3\">\n                        <div class=\"card-wrapper\" mbr-class=\"{'align-center': cardAlign==2,\n                                 'align-right': cardAlign==3, 'mbr-p-4': cardsAmount == 4 &amp;&amp; (showBorders || !transparentBg), 'mbr-p-5': cardsAmount < 4 &amp;&amp; (showBorders || !transparentBg)}\">\n                            <div class=\"card-img\">\n                                <amp-img class=\"mobirise-loader\" mbr-class=\"{'rounded': roundedImage, 'mbr-m-auto': cardAlign==2, 'mbr-ml-auto': cardAlign==3}\" src=\"@PROJECT_PATH@/assets/images/mbr-5-216x144.jpg\" layout=\"responsive\" width=\"570\" height=\"380\" alt=\"Mobirise\">\n                                    <div placeholder=\"\" class=\"placeholder\"></div>\n                                    <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/mbr-5-216x144.jpg\" alt=\"Mobirise\">\n                                </amp-img>\n                            </div>\n                            <div class=\"card-box\" mbr-if=\"showCardsTitle || showCardsText\">\n                                <p class=\"card-text mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-7\" mbr-if=\"showCardsText\" data-app-selector=\".card-text\">I have been a amateur fighter&nbsp; in the Houston, Texas area from 2008 and a registered member from 2010. I now teach Jujitsu and fight in the NAGA league. The uwmta showed me the other side of MMA and guided me through some of my first fight including Houston annual BEATDOWN fights.</p>\n                                <h3 class=\"card-subtitle mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".card-subtitle\" mbr-class=\"{'mbr-m-auto': cardAlign==2, 'mbr-ml-auto': cardAlign==3 }\">ROBERT ZELL<br>TX</h3>\n                            </div>\n                        </div>\n                    </div><div class=\"card last-child mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-6 mbr-col-xl-6':cardsAmount == 2 &amp;&amp; typeSlide == 'carousel', 'mbr-col-md-4':cardsAmount == 3 &amp;&amp; typeSlide == 'carousel', 'mbr-col-md-6 mbr-col-lg-3':cardsAmount == 4 &amp;&amp; typeSlide == 'carousel', 'mbr-col-md-12': cardsAmount == 1 ||  typeSlide != 'carousel', 'super-hide': slidersAmount < 5 &amp;&amp; typeSlide == 'carousel'}\" mbr-if=\"slidersAmount > 4\">\n                        <div class=\"card-wrapper\" mbr-class=\"{'align-center': cardAlign==2,\n                                 'align-right': cardAlign==3, 'mbr-p-4': cardsAmount == 4 &amp;&amp; (showBorders || !transparentBg), 'mbr-p-5': cardsAmount < 4 &amp;&amp; (showBorders || !transparentBg)}\">\n                            <div class=\"card-img\">\n                                <amp-img class=\"mobirise-loader\" mbr-class=\"{'rounded': roundedImage, 'mbr-m-auto': cardAlign==2, 'mbr-ml-auto': cardAlign==3}\" src=\"@PROJECT_PATH@/assets/images/nikki-216x162.jpg\" layout=\"responsive\" width=\"506.66666666666663\" height=\"380\" alt=\"Mobirise\">\n                                    <div placeholder=\"\" class=\"placeholder\"></div>\n                                    <img class=\"mbr-tem