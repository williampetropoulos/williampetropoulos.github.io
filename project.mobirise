        {
          "alias": "amp-blocks/components",
          "_isSecondary": true,
          "typeCompatible": "isAMP",
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".contacts-box .contacts-text:not(.last-child) when (@showContacts)": {
              "padding-bottom": "5px"
            },
            ".img-box when (@showLogo), (@showBrand)": {
              "align-items": "center"
            },
            ".img-wrap when (@showLogo) and (@showBrand) and (@showLogoSocial)": {
              "margin-right": "15px"
            },
            "& when (@showForm)": {
              ".mbr-section-btn": {
                "padding-bottom": "0"
              },
              ".field-input": {
                "color": "@labelsColor",
                "&:not([type=\"checkbox\"]):not([type=\"radio\"])": {
                  "background": "contrast(@labelsColor, darken(@labelsColor, 50%), #ffffff, 60%) !important",
                  "border-color": "lighten(@labelsColor, 88%)",
                  "&:focus": {
                    "border-color": "@secondaryColor"
                  }
                }
              },
              "input::placeholder": {
                "color": "lighten(@labelsColor, 46%)"
              }
            },
            ".social .iconfont-wrapper when (@showSocial) and (@showLogoSocial)": {
              "line-height": "(@iconsSize + 0.25rem)",
              "width": "~\"@{iconsSize}rem\"",
              "height": "~\"@{iconsSize}rem\"",
              "border-color": "@iconsColor !important",
              "color": "contrast(@iconsColor, #222222, #ffffff, 15%)",
              "background": "@iconsColor !important",
              "& when ((@iconsSize * 16) > 30)": {
                "@media (max-width: 767px)": {
                  "width": "30px",
                  "height": "30px",
                  "line-height": "34px"
                }
              }
            },
            ".social .amp-iconfont when (@showSocial) and (@showLogoSocial)": {
              "font-size": "(@iconsSize * 0.42rem)",
              "line-height": "(@iconsSize - 0.25rem)",
              "width": "(@iconsSize * 0.42rem)",
              "color": "contrast(@iconsColor, #222222, #ffffff, 15%)",
              "& when ((@iconsSize * 16) > 30)": {
                "@media (max-width: 767px)": {
                  "font-size": "(30px * 0.42)",
                  "width": "(30px * 0.42)",
                  "line-height": "26px"
                }
              },
              "&:hover": {
                "color": "@primaryColor !important"
              }
            },
            ".form-block when (@transparentForm) and (@showForm)": {
              "padding": "0"
            },
            ".form-block .mbr-overlay when not (@transparentForm) and (@showForm)": {
              "background-color": "@formBgColor",
              "opacity": "@formBgOpacity"
            },
            ".img-wrap when (@showLogo) and (@showLogoSocial)": {
              "width": "~\"@{widthLogo}%\"",
              "min-width": "60px"
            },
            "@media (max-width: 991px)": {
              ".mbr-row > .md-pb": {
                "padding-bottom": "30px"
              }
            },
            "@media (max-width: 767px)": {
              ".mbr-row > :not(.last-child)": {
                "padding-bottom": "30px"
              },
              ".form-block .field when (@showForm)": {
                "padding-bottom": "0"
              }
            },
            ".mbr-section-title:after when (@showLineTitle) and (@showTitle)": {
              "content": "\"\"",
              "display": "block",
              "width": "40px",
              "height": "1px",
              "margin-top": "10px",
              "background-color": "@primaryColor",
              "& when (@align = '2')": {
                "margin-left": "auto",
                "margin-right": "auto"
              },
              "& when (@align = '3')": {
                "margin-left": "auto",
                "@media (max-width: 767px)": {
                  "margin-right": "auto"
                }
              }
            },
            ".mbr-list .mbr-text when (@showCategories)": {
              "line-height": "1",
              "&:before when (@showMarkers)": {
                "content": "''",
                "display": "inline-block",
                "vertical-align": "middle",
                "width": "~\"@{markersSize}rem\"",
                "height": "~\"@{markersSize}rem\"",
                "border-radius": "50%",
                "margin-right": "15px",
                "background-color": "@markersColor"
              },
              "& when (@showEffect)": {
                "transition": "all 0.3s",
                "&:hover": {
                  "padding-left": "20px",
                  "color": "@primaryColor"
                }
              }
            },
            ".mbr-list .mbr-text": {
              "text-align": "left"
            }
          },
          "_name": "ampFooter17",
          "_customHTML": "<section class=\"ampFooter17\" group=\"Footers\" plugins=\"amp-main, amp-form, amp-mustache\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" condition=\"showTitle || (showContacts &amp;&amp; (showAddress || showPhone || showEmail)) || showCategories || (showForm &amp;&amp; (showTitle || showFormText)) || (showLogoSocial &amp;&amp; (showLogo || showSocial || showLogoText))\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"10\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"10\">\n        <input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\" condition=\"showTitle || (showContacts &amp;&amp; (showAddress || showPhone || showEmail)) || showCategories || (showForm &amp;&amp; (showTitle || showFormText)) || (showLogoSocial &amp;&amp; (showLogo || showSocial || showLogoText))\">\n    <header condition=\"showForm || showContacts || showCategories\">Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked=\"\" condition=\"showForm || showContacts || showCategories\">\n        <input type=\"checkbox\" title=\"Line Title\" name=\"showLineTitle\" condition=\" showTitle &amp;&amp; (showForm || showContacts || showCategories)\" checked=\"\">\n        <input type=\"checkbox\" title=\"Logo and Social\" name=\"showLogoSocial\" checked=\"\">\n        <input type=\"checkbox\" title=\"Contacts\" name=\"showContacts\" checked=\"\">\n        <input type=\"checkbox\" title=\"Categories\" name=\"showCategories\" checked=\"\">\n        <input type=\"checkbox\" title=\"Form\" name=\"showForm\" checked=\"\">\n        <select title=\"Align\" name=\"align\" condition=\"showTitle || (showContacts &amp;&amp; (showAddress || showPhone || showEmail)) || showCategories || (showForm &amp;&amp; (showTitle || showFormText)) || (showLogoSocial &amp;&amp; (showLogo || showSocial || showLogoText))\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected=\"\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n    <header condition=\"showLogoSocial\">Logo and Social</header>\n        <input type=\"checkbox\" name=\"showLogo\" title=\"Logo\" condition=\"showLogoSocial\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Width\" name=\"widthLogo\" min=\"10\" max=\"100\" value=\"10\" step=\"1\" condition=\"showLogoSocial &amp;&amp; showLogo\">\n        <input type=\"checkbox\" title=\"Show Brand Name\" name=\"showBrand\" condition=\"showLogoSocial\" checked=\"\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showLogoText\" checked=\"\" condition=\"showLogoSocial\"> \n        <input type=\"checkbox\" name=\"showSocial\" title=\"Social\" checked=\"\" condition=\"showLogoSocial\"> \n            <input type=\"range\" inline=\"\" title=\"Count\" name=\"socialCount\" min=\"1\" max=\"10\" value=\"3\" step=\"1\" condition=\"showSocial &amp;&amp; showLogoSocial\">\n            <input type=\"range\" inline=\"\" title=\"Size\" name=\"iconsSize\" min=\"1.5\" max=\"5\" step=\"0.1\" value=\"2.8\" condition=\"showSocial &amp;&amp; showLogoSocial\">\n            <input type=\"color\" title=\"Color\" name=\"iconsColor\" value=\"#4d4d4d\" condition=\"showSocial &amp;&amp; showLogoSocial\">\n    <header condition=\"showContacts\">Contacts</header>\n        <input type=\"checkbox\" name=\"showAddress\" title=\"Address\" checked=\"\" condition=\"showContacts\">\n        <input type=\"checkbox\" name=\"showPhone\" title=\"Phone\" checked=\"\" condition=\"showContacts\">\n        <input type=\"checkbox\" name=\"showEmail\" title=\"E-mail\" checked=\"\" condition=\"showContacts\">\n    <header condition=\"showCategories\">Categories</header>\n        <input type=\"range\" title=\"Count\" name=\"categoriesCount\" min=\"1\" max=\"10\" step=\"1\" value=\"8\" condition=\"showCategories\">\n        <input type=\"checkbox\" title=\"Markers\" name=\"showMarkers\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Size\" name=\"markersSize\" min=\"0.1\" max=\"1.5\" step=\"0.1\" value=\"0.5\" condition=\"showCategories &amp;&amp; showMarkers\">\n        <input type=\"color\" title=\"Color\" name=\"markersColor\" value=\"#ffffff\" condition=\"showMarkers &amp;&amp; showCategories\">\n        <input type=\"checkbox\" title=\"Show Effect Categorie\" name=\"showEffect\" checked=\"\">\n    <header condition=\"showForm\">Form</header>\n        <input type=\"checkbox\" title=\"Text\" name=\"showFormText\" condition=\"showForm\" checked=\"\">  \n        <input type=\"color\" title=\"Form Text Color\" name=\"labelsColor\" value=\"#000000\" condition=\"showForm\">\n        <input type=\"checkbox\" title=\"Transparent\" name=\"transparentForm\" condition=\"showForm\" checked=\"\">    \n            <input type=\"color\" title=\"Form Bg Color\" name=\"formBgColor\" value=\"#ffffff\" condition=\"!transparentForm &amp;&amp; showForm\">\n            <input type=\"range\" title=\"Form Bg Opacity\" name=\"formBgOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"1\" condition=\"!transparentForm &amp;&amp; showForm\">\n    <header>Background</header>\n        <input type=\"checkbox\" title=\"2 Background Color\" name=\"twoBgColor\" condition=\"bg.type == 'color'\"> \n        <input type=\"range\" inline=\"\" title=\"Width 2 Bg\" name=\"width2Bg\" min=\"1\" max=\"5\" step=\"1\" value=\"4\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">    \n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background9.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#4d4d4d\" selected=\"\">\n        </fieldset>\n   